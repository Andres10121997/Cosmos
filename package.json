{
  "license": "Apache",
  "browserslist": [
    ">= 0%",
    "android >= 1",
    "chrome >= 1",
    "edge >= 1",
    "Firefox >= 1",
    "ie >= 1",
    "ios_saf >= 1",
    "opera >= 1",
    "safari >= 1",
    "samsung >= 1"
  ],
  "scripts": {
    "css:d": "npx rimraf ./src/Style/css/Styles.css && npx sass --style=expanded ./src/Style/scss/Styles.scss:./src/Style/css/Styles.css",
    "css:p": "npx rimraf ./build/css/Cosmos.css && npx  npx postcss ./src/Style/css/Styles.css --output ./build/css/Cosmos.css",
    "jsdoc": "npx rimraf ./docs && npx jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "autoprefixer": "10.4.7",
    "jsdoc": "3.6.10"
  },
  "devDependencies": {
    "npm-sass": "3.1.0",
    "postcss": "8.4.14",
    "postcss-cli": "10.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.7.4"
  }
}
